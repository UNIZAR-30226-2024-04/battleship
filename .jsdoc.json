{
    "plugins": ["plugins/markdown"],
    "source": {
      "include": ["src","README.md"],
      "includePattern": ".+\\.js$",
      "excludePattern": "(node_modules/|docs|tests|config|coverage|examples)"
    },
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "opts": {
      "destination": "./docs/",
      "readme": "./README.md",
      "encoding": "utf8",
      "private": true,
      "recurse": true,
      "template": "./node_modules/docdash"
    },
    "docdash": {
      "search": true
    }
  }